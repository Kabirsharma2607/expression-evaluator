# Copy this file to .env and fill in your actual values
# DO NOT commit .env to git (it's in .gitignore)

# S3 Configuration
S3_POLICY_ENABLED=true
S3_POLICY_BUCKET=expression-evaluator-policies
AWS_REGION=ap-south-1
S3_POLICY_PREFIX=policies/

# AWS Credentials - GET THESE FROM AWS CONSOLE
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_access_key_here

# Optional S3 Settings
S3_CACHE_TTL_MINUTES=60
S3_REFRESH_INTERVAL_MS=3600000
S3_ENABLE_FALLBACK=true